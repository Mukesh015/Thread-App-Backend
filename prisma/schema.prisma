generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model User {
  id String @id @default(uuid())
  firstName String @map("first_Name")
  lastName String? @map("last_Name")
  profileImageURL String? @map("profile_image_url")
  email String @unique
  salt String
  password String
  
  @@map("users")
}